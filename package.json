{
  "name": "",
  "version": "",
  "description": "",
  "main": "dist",
  "scripts": {
    "_COMMENT": "Test scripts",
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint && yarn test",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors --compilers js:babel-core/register tests/*.run.js --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register tests/*.run.js --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "debug": "nodemon --inspect --debug-brk -w src --exec \"babel-node src --presets es2015,stage-0\""
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "",
  "author": "",
  "license": "",
  "dependencies": {
    "body-parser": "^1.13.3",
    "chai": "^3.5.0",
    "chance": "^1.0.6",
    "compression": "^1.5.2",
    "continuation-local-storage": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "faker": "^4.1.0",
    "graphql": "^0.8.0",
    "graphql-relay": "^0.4.3",
    "graphql-sequelize": "^5.3.1",
    "graphql-sequelize-crud": "^0.3.0",
    "helmet": "^3.5.0",
    "http-status": "^1.0.1",
    "joi": "10.0.6",
    "lodash": "^4.17.4",
    "method-override": "^2.3.8",
    "morgan": "^1.8.0",
    "nodemon": "^1.11.0",
    "pg": "^6.1.5",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^3.30.4",
    "sequelize-fixtures": "^0.5.6",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.2.0",
    "nodemon": "^1.9.2",
    "run-sequence": "^1.1.5",
    "supertest": "2.0.1",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.6.1"
  }
}
